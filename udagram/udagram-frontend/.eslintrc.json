{
    "env": {
        "browser": true,
        "es2021": true,
        "jasmine": true
    },
    "extends": [
        "eslint:recommended"
        
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "ignorePatterns": ["temp.js", "**/*.e2e-spec.ts"],
    "rules": {
        "indent": "off",
        "no-unused-vars": "off"
    }
    
}
